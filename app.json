    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f8a93b2b-99b3-4767-9603-ea5e93580027"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f8a93b2b-99b3-4767-9603-ea5e93580027"
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-1200533271102374~5300895420",
    "ios_app_id": "ca-app-pub-1200533271102374~6719872583"
  }
