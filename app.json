            "CFBundleTypeRole": "Editor",
            "CFBundleURLSchemes": [
              "com.zakatechsoftware.QuizAp"
            ]
          }
        ]
      },
      "buildNumber": "11"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.zakatechsoftware.QuizAp",
